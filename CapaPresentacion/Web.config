  <connectionStrings>
    <add name="AOCRConnection" connectionString="Host=172.20.16.55;Port=5432;Database=dgac_des;Username=root;Password=control;Pooling=true;MinPoolSize=1;MaxPoolSize=20;CommandTimeout=60;" providerName="Npgsql" />
  </connectionStrings>

  <appSettings>
    <add key="webpages:Enabled" value="false" />
    <add key="owin:AppStartup" value="CapaPresentacion.Startup" />
  </appSettings>
  
  <system.net>
    <mailSettings>
      <smtp from="no-reply@tusistema.com">
        <network host="smtp.tuservidor.com"
                 port="587"
                 userName="tu_usuario"
                 password="tu_contraseña"
                 enableSsl="true" />
